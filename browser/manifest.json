{
	"manifest_version": 2,
	"name": "iloveads",
	"version": "1.0",
	"description": "",
	"icons": {
		"48": "icons/icon-48.png"
	},
	"permissions": [
		"webRequest",
		"storage",
		"<all_urls>"
	],
	"background": {
		"scripts": ["js/background.js"]
	},
	"browser_action": {
		"default_icon": {
			"48": "icons/icon-48.png"
		},
		"default_popup": "popup.html"
	}
}
