{
  "manifest_version": 2,
  "name": "iloveads",
  "version": "1.0",
  "description": "",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://*/*"
  ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon-48.png"
  }
}
